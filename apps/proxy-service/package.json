{
  "name": "proxy-service",
  "version": "1.0.0",
  "description": "Cross-platform proxy service for family privacy routing",
  "main": "dist/family-proxy",
  "scripts": {
    "build": "mkdir -p dist && go build -o dist/family-proxy cmd/agent/main.go",
    "build:all": "./scripts/build.sh",
    "dev": "go run cmd/agent/main.go",
    "test": "go test ./... -v",
    "lint": "go fmt ./... && go vet ./...",
    "fmt": "go fmt ./...",
    "vet": "go vet ./...",
    "clean": "rm -rf dist/",
    "mod-tidy": "go mod tidy"
  },
  "keywords": ["proxy", "privacy", "family", "routing", "vpn"],
  "license": "UNLICENSED",
  "private": true
}